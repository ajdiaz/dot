# Set default values for all following accounts.
defaults
tls on
tls_trust_file /etc/ssl/certs/ca-certificates.crt
logfile /dev/null

# Account: connectical
account connectical
host smtp.gmail.com
port 587
from ajdiaz@connectical.com
auth on
user ajdiaz@connectical.com

# Set default account
account default : connectical
