# Set default values for all following accounts.
defaults
tls on
tls_trust_file /etc/ssl/certs/ca-certificates.crt
logfile /dev/null

# Account: ajdiaz.me
account ajdiaz.me
host mail.ajdiaz.me
port 587
from ajdiaz@ajdiaz.me
auth on
user ajdiaz
passwordeval "pass show mail.ajdiaz.me"

# Set default account
account default : ajdiaz.me
